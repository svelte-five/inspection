{
	"name": "@svelte-five/inspection",
	"version": "0.1.0",
	"description": "Dev mode, frontend logging library for svelte with support for elimination in production.",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./types.d.ts",
	"exports": {
		".": {
			"types": {
				"default": "./types.d.ts"
			},
			"import": "./dist/index.js",
			"require": "./dist/index.cjs",
			"default": "./dist/index.js"
		}
	},
	"scripts": {
		"lint": "eslint src",
		"format:check": "prettier src --check",
		"format": "prettier $(git diff --cached --name-only --diff-filter=ACMR src | sed 's| |\\\\ |g') --write --ignore-unknown",
		"build": "rollup --config rollup.config.js",
		"test": "vitest run",
		"prepare": "husky",
		"commit": "cz"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/svelte-five/inspection.git"
	},
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"license": "MIT",
	"author": {
		"name": "Murat GÃ¶zel",
		"email": "murat@gozel.com.tr",
		"url": "https://muratgozel.com.tr"
	},
	"keywords": [
		"frontend-logging",
		"svelte"
	],
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	},
	"devDependencies": {
		"@babel/cli": "^7.26.4",
		"@babel/core": "^7.26.9",
		"@babel/preset-typescript": "^7.26.0",
		"@eslint/js": "^9.21.0",
		"@rollup/plugin-babel": "^6.0.4",
		"@rollup/plugin-commonjs": "^28.0.3",
		"@rollup/plugin-strip": "^3.0.4",
		"@rollup/plugin-typescript": "^12.1.2",
		"commitizen": "^4.3.1",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^9.21.0",
		"globals": "^16.0.0",
		"husky": "^9.1.7",
		"prettier": "^3.5.2",
		"rollup": "^4.35.0",
		"semantic-release": "^24.2.3",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.25.0",
		"vite": "^6.2.1",
		"vitest": "^3.0.7"
	}
}
